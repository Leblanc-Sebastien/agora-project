{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{vue,ts,js,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{vue,ts,js,json,css,md}\"",
    "mobile:build": "nuxt generate && npx cap sync",
    "mobile:android": "npm run mobile:build && npx cap open android",
    "mobile:ios": "npm run mobile:build && npx cap open ios",
    "mobile:add-android": "npx cap add android",
    "mobile:add-ios": "npx cap add ios",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/ios": "^7.4.4",
    "@internationalized/date": "^3.9.0",
    "@nuxt/ui": "^3.3.3",
    "@pinia/nuxt": "^0.11.3",
    "@tailwindcss/vite": "^4.1.13",
    "nuxt": "^4.1.1",
    "pinia": "^3.0.4",
    "tailwindcss": "^4.1.13",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.19.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.35.0",
    "eslint-plugin-vue": "^10.4.0",
    "happy-dom": "^18.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "vitepress": "^1.6.4",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0"
  }
}
